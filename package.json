{
  "name": "ct-iot-things-lambda",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "build": "./node_modules/typescript/bin/tsc  **/*.ts",
    "lint": "eslint . --ext .ts",
    "depcheck": "depcheck",
    "test": "NODE_ENV=test AWS_ACCESS_KEY_ID=1 AWS_SECRET_ACCESS_KEY=2 AWS_REGION=localhost mocha -r ts-node/register --config ./test/mocharc.json ./test/**/*.test.ts",
    "dev:package-create": "zip -r ../ct-iot-serverless-infrastructure/terraform/development/create-thing-development-lambda-function.zip ./lambda-create",
    "dev:package-read": "zip -r ../ct-iot-serverless-infrastructure/terraform/development/read-thing-development-lambda-function.zip ./lambda-read",
    "dev:package-all": "npm run dev:package-create && npm run dev:package-read",
    "stage:package-create": "zip -r ../ct-iot-serverless-infrastructure/terraform/staging/create-thing-staging-lambda-function.zip ./lambda-create",
    "stage:package-read": "zip -r ../ct-iot-serverless-infrastructure/terraform/staging/read-thing-staging-lambda-function.zip ./lambda-read",
    "stage:package-all": "npm run stage:package-create && npm run stage:package-read",
    "prod:package-create": "zip -r ../ct-iot-serverless-infrastructure/terraform/production/create-thing-production-lambda-function.zip ./lambda-create",
    "prod:package-read": "zip -r ../ct-iot-serverless-infrastructure/terraform/production/read-thing-production-lambda-function.zip ./lambda-read",
    "prod:package-all": "npm run prod:package-create && npm run prod:package-read"
  },
  "engines": {
    "node": ">=16.15"
  },
  "devDependencies": {
    "@aws-sdk/client-dynamodb": "^3.150.0",
    "@aws-sdk/lib-dynamodb": "^3.216.0",
    "@types/aws-lambda": "^8.10.109",
    "@types/chai": "^4.3.4",
    "@types/mocha": "^10.0.0",
    "@types/node": "^18.0.3",
    "@types/sinon": "^10.0.13",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.42.1",
    "aws-lambda": "^1.0.7",
    "chai": "^4.3.7",
    "depcheck": "^1.4.3",
    "eslint": "^8.18.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.1.0",
    "mocha": "^10.1.0",
    "prettier": "^2.7.1",
    "sinon": "^14.0.2",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4",
    "uuid": "^8.3.2"
  }
}
